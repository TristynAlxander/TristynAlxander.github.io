<Equation version="1.0" name="Moment of Inertia for a Solid Cylinder about an axis Perpendicular to its Symmetric Axis">
	<LaTeX>
		I = \frac{1 m_{T} r^2}{4} + \frac{1 m_{T} \ell^2}{12}
	</LaTeX>
	<Variables>
		<Var	symbol="I"			math_object="Scalar"		 				unit="kg m^2"				name="Moment of Inertia, Angular Mass"	/>
		<Var 	symbol="m"			math_object="Scalar"						unit="kg"					name="Mass, Inertia"					/>
		<Var 	symbol="r"			math_object="Scalar"						unit="m"					name="Radius, Position"					/>
		<Var 	symbol="\ell"		math_object="Scalar"						unit="m"					name="Length"							/>
	</Variables>
	<Assumptions></Assumptions>
</Equation>