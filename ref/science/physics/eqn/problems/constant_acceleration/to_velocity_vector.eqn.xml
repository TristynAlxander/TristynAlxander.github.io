<Equation version="1.0" name="Constant Acceleration to Velocity (Vector)">
	<LaTeX> \vec{v} = \vec{v}_{0} + \vec{a} t </LaTeX>
	<Variables>
		<Var 	symbol="\vec{a}"		math_object="Vector"		unit="m s^{-2}"			name="Acceleration"							/>
		<Var 	symbol="\vec{v}"		math_object="Vector"		unit="m s^{-1}"			name="Velocity"								/>
		<Var 	symbol="t"				math_object="Scalar"		unit="s"				name="Time"									/>
	</Variables>
	<Assumptions>
		<Ass text="Constant Acceleration"/>
	</Assumptions>
</Equation>