<Equation version="1.0" name="Angle Displacement under Constant Angular Acceleration">
	<LaTeX> \theta - \theta_{0} = \frac{\omega^{2} - \omega_{0}^{2}}{2 \alpha_a} </LaTeX>
	<Variables>
		<Var 	symbol="\theta"				math_object=""				unit=""				name="Angle"					/>
		<Var 	symbol="\omega"				math_object=""				unit="s^{-1}"		name="Angular Velocity"			/>
		<Var 	symbol="\alpha_{a}"			math_object=""				unit="s^{-2}"		name="Angular Acceleration"		/>
	</Variables>
	<Assumptions>
		<Ass text="Constant Angular Acceleration"/>
	</Assumptions>
</Equation>