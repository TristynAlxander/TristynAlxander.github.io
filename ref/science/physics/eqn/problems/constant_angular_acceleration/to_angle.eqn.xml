<Equation version="1.0" name="Constant Angular Acceleration to Angle">
	<LaTeX>
		\theta = \theta_{0} + \omega_{0} t + \frac{\alpha_{a} t^2}{2}
	</LaTeX>
	<Variables>
		<Var 	symbol="\theta"				math_object="Scalar"		unit=""						name="Angle"							/>
		<Var 	symbol="\omega"				math_object="Scalar"		unit="s^{-1}"				name="Angular Velocity"					/>
		<Var 	symbol="\alpha_{a}"			math_object="Scalar"		unit="s^{-2}"				name="Angular Acceleration"				/>
		<Var 	symbol="t"					math_object="Scalar"		unit="s"					name="Time"								/>
	</Variables>
	<Assumptions>
		<Ass text="Constant Angular Acceleration"/>
	</Assumptions>
</Equation>