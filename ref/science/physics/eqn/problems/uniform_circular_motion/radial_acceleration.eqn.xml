<Equation version="1.0" name="Radial Acceleration under Uniform Circular Motion">
	<LaTeX> \alpha_{r} = \frac{v^{2}}{r} = \omega^{2} r </LaTeX>
	<Variables>
		<Var 	symbol="\alpha_{r}"		math_object=""				unit="m s^{-2}"		name="Radial Acceleration"		/>
		<Var 	symbol="\vec{v}"		math_object=""				unit="m s^{-1}"		name="Velocity"					/>
		<Var 	symbol="\omega"			math_object=""				unit="s^{-1}"		name="Angular Velocity"			/>
		<Var 	symbol="r"				math_object=""				unit="m"			name="Radius"					/>	
	</Variables>
	<Assumptions>
		<Ass text="Circular Motion"/>
		<Ass text="Constant Velocity"/>
	</Assumptions>
</Equation>