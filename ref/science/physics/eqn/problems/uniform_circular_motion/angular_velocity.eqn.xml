<Equation version="1.0" name="Angular Velocity under Uniform Circular Motion">
	<LaTeX> \omega = \frac{v}{r} </LaTeX>
	<Variables>
		<Var 	symbol="\omega"				math_object=""				unit="s^{-1}"		name="Angular Velocity"			/>
		<Var 	symbol="v"					math_object=""				unit="m s^{-1}"		name="Velocity"					/>	
		<Var 	symbol="r"					math_object=""				unit="m"			name="Radius"					/>	
	</Variables>
	<Assumptions>
		<Ass text="Circular Motion"/>
		<Ass text="Constant Velocity"/>
	</Assumptions>
</Equation>