<Equation version="1.0" name="Torque under Uniform Circular Motion">
	<LaTeX>
		\tau = I \alpha_a
	</LaTeX>
	<Variables>
		<Var 	symbol="\vec{\tau}, M"				math_object="Vector"				unit="N m"				name="Torque, Moment of Force"				/>
		<Var 	symbol="I"							math_object="Scalar"				unit="kg"				name="Moment of Inertia, Angular Mass"		/>
		<Var 	symbol="\vec{\alpha}_{a}"			math_object="Vector"				unit="N"				name="Force"								/>
	</Variables>
	<Assumptions>
		<Ass text="Circular Motion"/>
		<Ass text="Constant Velocity"/>
	</Assumptions>
</Equation>