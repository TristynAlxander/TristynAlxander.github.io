<Equation version="1.0" name="Angular Momentum under Uniform Circular Motion">
	<LaTeX> \vec{L} = I \omega </LaTeX>
	<Variables>
		<Var 	symbol="\vec{L}"			math_object="Vector"				unit="J s"				name="Angular Momentum"					/>
		<Var 	symbol="I"					math_object="Scalar"				unit="kg m^2"			name="Moment of Inertia (Angular Mass)"	/>
		<Var 	symbol="\vec{\omega}"		math_object="Rotational Vector"		unit="s^{-1}"			name="Angular Velocity"					/>
	</Variables>
	<Assumptions>
		<Ass text="Circular Motion"/>
		<Ass text="Constant Velocity"/>
	</Assumptions>
</Equation>