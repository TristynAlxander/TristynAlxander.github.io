<Equation version="1.0" name="Time of Flight">
	<LaTeX> 
		v_{y} = v_{y,0} + a_{y} t				\\
		v_{y,t_{1/2}} = 0						\\
		0 = v_{y,0} + a_{y} t_{1/2}				\\
		t_{1/2} = - \frac{v_{y,0}}{a_{y}}		\\
		t = 2 t_{1/2}							\\
		t = - 2 \frac{v_{y,0}}{a_{y}}			\\
		v_{y,0} = v_{0,x-y} \sin(\theta)		\\
		a_{y} = -g								\\
		t = \frac{2 v_{0} \sin(\theta)}{g} 		\\
	</LaTeX>
	<Variables>
		<Var 	symbol="t"				math_object="Scalar"				unit="s"				name="Time of Flight"							/>
		<Var 	symbol="v"				math_object="Scalar"				unit="m s^{-1}"			name="Velocity"									/>
		<Var 	symbol="a"				math_object="Scalar"				unit="m s^{-2}"			name="Acceleration"							/>
		<Var 	symbol="\theta"			math_object="Scalar"				unit="^{(R)}"			name="angle"									/>
		<Var 	symbol="g"				math_object="9.80665"			unit="m s^{-2}"			name="Near-Earth Gravitational Acceleration"	/>
	</Variables>
	<Assumptions></Assumptions>
</Equation>