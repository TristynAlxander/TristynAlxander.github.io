<Equation version="1.0" name="Definition of Work (via Force)">
	<LaTeX>
		W \equiv \int_{t_{i}}^{t_{f}} \vec{F} \cdot \vec{v} dt = \int_{\vec{r}_{i}}^{\vec{r}_{f}} \vec{F} \cdot d\vec{r}
	</LaTeX>
	<Variables>
		<Var 	symbol="t"									math_object="Scalar"						unit="s"					name="Time"								/>
		<Var 	symbol="\vec{v}"							math_object="Vector"						unit="m s^{-1}"				name="Velocity"							/>
		<Var 	symbol="\vec{F}"							math_object="Vector"						unit="N"					name="Force"							/>
		<Var 	symbol="\vec{r}, \vec{x},\vec{\ell}"		math_object="Vector"						unit="m"					name="Position, Radius"					/>
		<Var 	symbol="W"									math_object="Scalar"						unit="J"					name="Work"								/>
	</Variables>
	<Assumptions></Assumptions>
</Equation>