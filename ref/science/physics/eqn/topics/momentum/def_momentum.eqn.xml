<Equation version="1.0" name="Definition of Momentum">
	<LaTeX>
		\vec{p} = m \vec{v}
	</LaTeX>
	<Variables>
		<Var 	symbol="\vec{v}"							math_object="Vector"				unit="m s^{-1}"				name="Velocity"							/>
		<Var 	symbol="m"									math_object="Scalar"				unit="kg"					name="Mass, Inertia"					/>
		<Var 	symbol="\vec{p}"							math_object="Vector"				unit="kg m s^{-1}"			name="Momentum"							/>
	</Variables>
	<Assumptions></Assumptions>
</Equation>