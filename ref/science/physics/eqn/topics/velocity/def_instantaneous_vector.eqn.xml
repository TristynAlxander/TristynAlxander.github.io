<Equation version="1.0" name="Definition of Instantaneous Velocity (Position to Velocity)">
	<LaTeX> \vec{v} \equiv \frac{d \vec{r}}{d t} </LaTeX>
	<Variables>
		<Var 	symbol="\vec{v}"		math_object="Vector"				unit="m s^{-1}"			name="Velocity"								/>
		<Var 	symbol="\vec{r}"		math_object="Vector"				unit="m"				name="Position, Radius"						/>
		<Var 	symbol="t"				math_object="Scalar"				unit="s"				name="Time"									/>
	</Variables>
	<Assumptions></Assumptions>
</Equation>