<Equation version="1.0" name="Relativistic Newton's Second Law">
	<LaTeX>
	F_{Net External} = \frac{d(mv)}{dt} = m \frac{dv}{dt} + v \frac{dm}{dt}
	</LaTeX>
	<Variables>
		<Var 	symbol="\vec{F}"			math_object="Vector"			unit="N"			name="Force"					/>
		<Var 	symbol="\vec{v}"			math_object="Vector"						unit="m s^{-1}"				name="Velocity"							/>
		<Var 	symbol="m"					math_object="Vector"			unit="kg"			name="Mass (Inertia)"			/>
	</Variables>
	<Assumptions></Assumptions>
</Equation>