<Equation version="1.0" name="Relativistic Newton's Second Law">
	<LaTeX>
	F_{Net External} = \frac{d(mv)}{dt} = m \frac{dv}{dt} + v \frac{dm}{dt}
	</LaTeX>
	<Variables>
		<Var 	symbol="\vec{F}"			mathObject="Vector"			unit="N"			name="Force"					measurands=""		dependencies=""/>
		<Var 	symbol="\vec{v}"							mathObject="Vector"						unit="m s^{-1}"				name="Velocity"							measurands=""		dependencies=""/>
		<Var 	symbol="m"					mathObject="Vector"			unit="kg"			name="Mass (Inertia)"			measurands=""		dependencies=""/>
	</Variables>
</Equation>