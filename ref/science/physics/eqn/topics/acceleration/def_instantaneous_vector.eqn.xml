<Equation version="1.0" name="Definition of Instantaneous Vector Acceleration (Position & Velocity to Acceleration)">
	<LaTeX> \vec{a} \equiv  \frac{d \vec{v}}{dt} \equiv  \frac{d^{2} \vec{r}}{dt^{2}} </LaTeX>
	<Variables>
		<Var 	symbol="\vec{a}"		math_object="Vector"				unit="m s^{-2}"			name="Acceleration"							/>
		<Var 	symbol="\vec{v}"		math_object="Vector"				unit="m s^{-1}"			name="Velocity"								/>
		<Var 	symbol="\vec{r}"		math_object="Vector"				unit="m"				name="Position, Radius"						/>
		<Var 	symbol="t"				math_object="Scalar"				unit="s"				name="Time"									/>
	</Variables>
	<Assumptions></Assumptions>
</Equation>