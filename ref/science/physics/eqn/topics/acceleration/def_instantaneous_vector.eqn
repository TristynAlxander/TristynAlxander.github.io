<Equation version="1.0" name="Definition of Instantaneous Vector Acceleration (Position & Velocity to Acceleration)">
	<LaTeX> \vec{a} \equiv  \frac{d \vec{v}}{dt} \equiv  \frac{d^{2} \vec{r}}{dt^{2}} </LaTeX>
	<Variables>
		<Var 	symbol="\vec{a}"		mathObject="Vector"				unit="m s^{-2}"			name="Acceleration"							measurands=""		dependencies=""/>
		<Var 	symbol="\vec{v}"		mathObject="Vector"				unit="m s^{-1}"			name="Velocity"								measurands=""		dependencies=""/>
		<Var 	symbol="\vec{r}"		mathObject="Vector"				unit="m"				name="Position, Radius"						measurands=""		dependencies=""/>
		<Var 	symbol="t"				mathObject="Scalar"				unit="s"				name="Time"									measurands=""		dependencies=""/>
	</Variables>
</Equation>