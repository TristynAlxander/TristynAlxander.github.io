<Equation version="1.0" name="Acceleration to Position (Vector)">
	<LaTeX> \vec{r} = \int \int \vec{a} dt dt = \vec{r}_{0} + \int \left( \vec{v}_{0} + \int \vec{a} dt \right) dt </LaTeX>
	<Variables>
		<Var 	symbol="\vec{r}"		math_object="Vector"				unit="m"				name="Position"								/>
		<Var 	symbol="\vec{a}"		math_object="Vector"				unit="m s^{-2}"			name="Acceleration"							/>
		<Var 	symbol="\vec{v}"		math_object="Vector"				unit="m s^{-1}"			name="Velocity"								/>
		<Var 	symbol="t"				math_object="Scalar"				unit="s"				name="Time"									/>
	</Variables>
	<Assumptions></Assumptions>
</Equation>